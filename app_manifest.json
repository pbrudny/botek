      "bot": [
        "channels:read",
        "chat:write",
        "files:read",
        "groups:read",
        "im:read",
        "mpim:read",
        "reactions:read",
        "reactions:write",
        "usergroups:read",
        "usergroups:write",
        "users.profile:read",
        "users:read",
        "incoming-webhook",
        "commands"
